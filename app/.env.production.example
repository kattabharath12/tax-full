
# Production Environment Variables for Railway Deployment

# Database Configuration (Railway will provide this automatically)
DATABASE_URL="postgresql://username:password@host:port/database"

# NextAuth Configuration
NEXTAUTH_URL="https://your-app-name.railway.app"
NEXTAUTH_SECRET="your-super-secure-secret-key-minimum-32-characters"

# AbacusAI Integration (Required)
ABACUSAI_API_KEY="your_abacusai_api_key_here"

# Google Document AI Configuration (Optional - will fallback to LLM if not configured)
GOOGLE_CLOUD_PROJECT_ID="your-google-cloud-project-id"
GOOGLE_CLOUD_LOCATION="us"
GOOGLE_CLOUD_W2_PROCESSOR_ID="your-w2-processor-id"
GOOGLE_CLOUD_1099_PROCESSOR_ID="your-1099-processor-id"
# For Railway, encode your service account JSON as base64 and use this variable
GOOGLE_APPLICATION_CREDENTIALS_JSON="base64-encoded-service-account-json-content"

# Production Settings
NODE_ENV="production"
NEXT_TELEMETRY_DISABLED="1"

# Optional: Custom domain configuration
# NEXTAUTH_URL="https://your-custom-domain.com"
